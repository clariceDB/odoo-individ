<?xml version="1.0"?>
<odoo>
    <menuitem id="music_menu"
              name="Music" />

    <!--songs-->
    <act_window id="action_songs"
      name="Song List"
      res_model="music.song"
      view_mode="tree,form"
	/>

    <menuitem id="menu_songs"
              name="Songs"
              parent="music_menu"
              action="action_songs"
    />

    <!--album-->
    <act_window id="action_album"
      name="Album List"
      res_model="music.album"
      view_mode="tree,form"
	/>

    <menuitem id="menu_albums"
              name="Albums"
              parent="music_menu"
              action="action_album"
    />

    <!--artist-->
    <act_window id="action_artist"
      name="Artist List"
      res_model="music.artist"
      view_mode="tree,form"
	/>

    <menuitem id="menu_artist"
              name="Artists"
              parent="music_menu"
              action="action_artist"
    />

  <!--genre-->
    <act_window id="action_genre"
      name="Genre List"
      res_model="music.genre"
      view_mode="tree,form"
	/>

    <menuitem id="menu_genre"
              name="Genres"
              parent="music_menu"
              action="action_genre"
    />

    <!--liked_list-->
    <act_window id="action_liked_list"
      name="Liked List"
      res_model="music.liked_list"
      view_mode="tree,form"
	/>

    <menuitem id="menu_liked_list"
              name="Liked List"
              parent="music_menu"
              action="action_liked_list"
    />
</odoo>
